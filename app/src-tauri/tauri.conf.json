{
  "productName": "Hubio",
  "version": "0.11.0",
  "identifier": "com.tauri.dev",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:5173",
    "beforeDevCommand": "pnpm run dev",
    "beforeBuildCommand": "pnpm run build"
  },
  "plugins": {
    "structure-manager": {
      "document": {
        "files": [],
        "dirs": {
          "Hubio": {
            "options": {
              "repair": true
            },
            "files": [],
            "dirs": {
              "projects": {
                "options": {
                  "repair": true
                },
                "files": [],
                "dirs": {}
              }
            }
          }
        }
      }
    }
  },
  "app": {
    "windows": [
      {
        "title": "Hubio",
        "width": 800,
        "height": 600,
        "minWidth": 800,
        "minHeight": 600,
        "resizable": true,
        "fullscreen": false,
        "transparent": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
